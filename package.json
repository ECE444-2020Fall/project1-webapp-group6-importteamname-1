{
  "name": "chefcopilot",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.5.0",
    "classnames": "2.2.6",
    "enzyme-adapter-react-16": "^1.15.5",
    "fs-extra": "9.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "concurrently": "5.2.0",
    "enzyme": "^3.11.0"
  },
  "scripts": {
    "start": "concurrently \"yarn start-frontend || npm run start-frontend\" \"yarn start-backend || npm run start-backend\"",
    "start-backend": "python3 server/server.py || python server/server.py || py -3 server/server.py",
    "install-requirements": "pip install -r server/requirements.txt",
    "start-frontend": "react-scripts start",
    "build": "node ./buildScript",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.js src/**/*.jsx"
  },
  "proxy": "http://localhost:3001",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
